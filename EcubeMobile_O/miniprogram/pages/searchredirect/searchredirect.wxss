.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td1 {
    width:10%;
    justify-content: center;
    text-align: center;
    text-overflow: ellipsis;
    word-wrap: break-word;
}
.td2 {
    width:25%;
    justify-content: center;
    text-align: center;
    text-overflow: ellipsis;
    word-wrap: break-word;
}
.td3 {
    width:40%;
    justify-content: center;
    text-align: center;
    text-overflow: ellipsis;
    word-wrap: break-word;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 20%;
  justify-content: center;
  background: gray;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.th2 {
  width: 40%;
  justify-content: center;
  background: gray;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}

.content{
  margin: left;
  color:  rgba(17, 21, 36, 0.6) ;
  font-size: 12px;
  font-weight: bold;
}